<?xml version="1.0"?>
<block>
  <name>Interleaved Reed-Solomon decoders</name>
  <key>ao40_rs_decoder</key>
  <category>[ao40]</category>
  <import>import ao40</import>
  <make>ao40.rs_decoder($verbose)</make>
  <param>
    <name>Verbose</name>
    <key>verbose</key>
    <type>bool</type>
     <option>
       <name>Yes</name>
       <key>True</key>
     </option>
     <option>
       <name>No</name>
       <key>False</key>
     </option>
  </param>

  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
